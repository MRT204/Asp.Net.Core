@model  IEnumerable<NetCoreMVCLab03.Models.Home>
    @foreach (var item in Model)
            {
                <div class="col-xs-3 col-sm-3 col-md-3 col-lg-3">
                    <div class="thumbnail">
                        <img src="@item.Image" alt="" >
                        <div class="Caption">
                            <p>
                                Price: @item.Price
                            </p>
                        </div>
                    </div>
                </div>
            }
